package mx.com.iusacell.services.miiusacell.vo;

public class RespuestaServiciosVO  implements java.io.Serializable
{
    private static final long serialVersionUID = 201511031751L;

    private boolean respuesta;
    private int     folioPreactivacion;
    private int     codigo;
    private String  mensaje;
    private int     idTx;
    
    
    public boolean isRespuesta()
    {
        return respuesta;
    }
    public void setRespuesta(final boolean respuesta)
    {
        this.respuesta = respuesta;
    }
    public int getFolioPreactivacion()
    {
        return folioPreactivacion;
    }
    public void setFolioPreactivacion(final int folioPreactivacion)
    {
        this.folioPreactivacion = folioPreactivacion;
    }
    public int getCodigo()
    {
        return codigo;
    }
    public void setCodigo(final int codigo)
    {
        this.codigo = codigo;
    }
    public String getMensaje()
    {
        return mensaje;
    }
    public void setMensaje(final String mensaje)
    {
        this.mensaje = mensaje;
    }
    public int getIdTx()
    {
        return idTx;
    }
    public void setIdTx(final int idTx)
    {
        this.idTx = idTx;
    }
}