}

public void setVigencia(final int vigencia) {
	this.vigencia = vigencia;
}

public String getUnidad() {
	return unidad;
}

public void setUnidad(final String unidad) {
	this.unidad = unidad;
}

public String getPeriodicidad() {
	return periodicidad;
}

public void setPeriodicidad(final String periodicidad) {
	this.periodicidad = periodicidad;
}

public String getPeriodicidadDesc() {
	return periodicidadDesc;
}

public void setPeriodicidadDesc(final String periodicidadDesc) {
	this.periodicidadDesc = periodicidadDesc;
}

public String getCodigo() {
	return codigo;
}

public void setCodigo(final String codigo) {
	this.codigo = codigo;
}




}