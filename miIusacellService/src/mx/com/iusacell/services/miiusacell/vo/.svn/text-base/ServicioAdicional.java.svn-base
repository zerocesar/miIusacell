package mx.com.iusacell.services.miiusacell.vo;


public class ServicioAdicional {
  
  private int servicioAdicionalId;
  private int idServicio;
  private int identificadorServicio;
  private String servicioAdicional;
  private String servicioAdicionalDesc;
  private long costo;
  private long monto;
  private String codigoDevengado;
  private int vigencia;
  private String unidad;
  private String periodicidad;
  private String periodicidadDesc;
  private String mensaje;
  private String codigo;
  
  
  

public String getMensaje() {
	return mensaje;
}

public void setMensaje(final String mensaje) {
	this.mensaje = mensaje;
}


public int getServicioAdicionalId() {
	return servicioAdicionalId;
}

public void setServicioAdicionalId(final int servicioAdicionalId) {
	this.servicioAdicionalId = servicioAdicionalId;
}

public int getIdServicio() {
	return idServicio;
}

public void setIdServicio(final int idServicio) {
	this.idServicio = idServicio;
}


public int getIdentificadorServicio() {
	return identificadorServicio;
}

public void setIdentificadorServicio(final int identificadorServicio) {
	this.identificadorServicio = identificadorServicio;
}

public String getServicioAdicional() {
	return servicioAdicional;
}

public void setServicioAdicional(final String servicioAdicional) {
	this.servicioAdicional = servicioAdicional;
}

public String getServicioAdicionalDesc() {
	return servicioAdicionalDesc;
}

public void setServicioAdicionalDesc(final String servicioAdicionalDesc) {
	this.servicioAdicionalDesc = servicioAdicionalDesc;
}


public String getCodigoDevengado() {
	return codigoDevengado;
}

public void setCodigoDevengado(final String codigoDevengado) {
	this.codigoDevengado = codigoDevengado;
}



public long getCosto() {
	return costo;
}

public void setCosto(final long costo) {
	this.costo = costo;
}

public long getMonto() {
	return monto;
}

public void setMonto(final long monto) {
	this.monto = monto;
}

public int getVigencia() {
