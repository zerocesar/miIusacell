package mx.com.iusacell.services.miiusacell.vo;

public class CargoVO {	
	private double    monto;	
	private int       mesesSinIntereses;
	private String    formaPago;
	private String    refBancaria;
	private String    concepto;
		
	public double getMonto() {
		return monto;
	}
	public void setMonto(final double monto) {
		this.monto = monto;
	}
	public int getMesesSinIntereses() {
		return mesesSinIntereses;
	}
	public void setMesesSinIntereses(final int mesesSinIntereses) {
		this.mesesSinIntereses = mesesSinIntereses;
	}	
	public String getFormaPago() {
		return formaPago;
	}
	public void setFormaPago(final String formaPago) {
		this.formaPago = formaPago;
	}
	public String getRefBancaria() {
		return refBancaria;
	}
	public void setRefBancaria(final String refBancaria) {
		this.refBancaria = refBancaria;
	}
	public String getConcepto() {
		return concepto;
	}
	public void setConcepto(final String concepto) {
		this.concepto = concepto;
	}	
}
