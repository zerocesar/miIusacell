package mx.com.iusacell.services.miiusacell.arquitectura;

import mx.com.iusacell.services.miiusacell.vo.*;

public class FlujoAccesos {
	  private String mensaje="";
	  private int codigo=0;
	  private DatosLogin datos=new DatosLogin();
	  private String llaveRecup;
	  private String correo;
	public String getMensaje() {
		return mensaje;
	}
	public void setMensaje(String mensaje) {
		this.mensaje = mensaje;
	}
	public int getCodigo() {
		return codigo;
	}
	public void setCodigo(int codigo) {
		this.codigo = codigo;
	}
	public DatosLogin getDatos() {
		return datos;
	}
	public void setDatos(DatosLogin datos) {
		this.datos = datos;
	}
	public String getLlaveRecup() {
		return llaveRecup;
	}
	public void setLlaveRecup(String llaveRecup) {
		this.llaveRecup = llaveRecup;
	}
	public String getCorreo() {
		return correo;
	}
	public void setCorreo(String correo) {
		this.correo = correo;
	}

	  
	  
	  
}
